                 <span style={{paddingLeft:10}}>${item.price[index]}</span>\n                            </div>\n                            <p className=\"details\">{item.description[index]}</p>\n\n                            <div className=\"row\">\n                                <span style={{color:'darkgoldenrod',paddingLeft:'10px'}}> Rating : {item.rating[index]}</span>\n                            </div>\n                        </div>\n\n                        {/*<div className=\"colors\">*/}\n                        {/*    {*/}\n                        {/*        item.colors.map((color,index)=>(*/}\n                        {/*            <button style={{background: color}} key={index}></button>*/}\n                        {/*            ))*/}\n                        {/*    }*/}\n\n                        {/*</div>*/}\n                        <div className=\"thumb\" ref={this.myRef}>\n                            {\n                                item.src.map((img,index)=>(\n                                    <img src={img} alt=\"\"  key={index}\n                                    onClick={()=>this.handleTab(index)}\n                                    />\n                                ))\n                            }\n                        </div>\n                        <button className=\"cart\" onClick={this.handleCLick.bind(this)}>Add To cart</button>\n                    </div>\n                ))\n            }\n        </div>\n            </React.Fragment>\n    );\n  };\n}\n\nexport default App;\n","/Users/manas/Documents/Projects/saatva/saatva/src/components/NavBar.js",["21"],"import React,{Component} from \"react\";\nimport {Container,Navbar} from 'react-bootstrap';\n\nlet ItemCount=0;\nclass NavBar extends Component{\n    render() {\n        return (\n            <React.Fragment>\n                <link rel=\"stylesheet\" href=\"//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css\"></link>\n                <Navbar expand